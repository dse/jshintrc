{
    // enforcing options
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "esversion": 5,            // 3, 5, 6, 7, 8, 9, 10, 11
    "forin": true,
    "freeze": true,
    "futurehostile": true,
    "globals": {},
    "latedef": "nofunc",        // true, false, "nofunc"
    "leanswitch": true,
    "maxcomplexity": 999999,
    "maxdepth": 999999,
    "maxerr": 50,
    "maxparams": 999999,
    "maxstatements": 999999,
    "noarg": true,
    "nocomma": true,
    "nonbsp": true,
    "nonew": true,
    "noreturnawait": true,
    "predef": [],
    "regexpu": false,
    "shadow": true,             // true, false or "inner", "outer"
    "singleGroups": false,
    "strict": "global",         // true, false, "global", "implied"
    "trailingcomma": false,
    "undef": true,
    "unused": true,             // true, false, "vars", "strict"
    "varstmt": false
}
